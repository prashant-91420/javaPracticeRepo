package p1;

public class B {
	
	public static void main(String[] args) {
		
		System.out.println("Hello");
		
		String str = "vivek";
		char [] arr =  str.toCharArray();
		
		int sno = 0;
		int eno = arr.length-1;
		char temp;
		while(sno<eno) {
			temp = arr[sno];
			arr[sno] = arr[eno];
			arr[eno] = temp;
		}
		
	}

}
